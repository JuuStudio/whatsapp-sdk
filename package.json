{
  "name": "@juutech/whatsapp-sdk",
  "version": "1.2.0",
  "description": "Modern TypeScript SDK for WhatsApp Cloud API v22.0",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist",
    "LICENSE",
    "README.md"
  ],
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "build": "rimraf dist && tsc",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:ci": "jest --ci --coverage",
    "test:debug": "node --inspect-brk ./node_modules/jest/bin/jest.js --runInBand",
    "lint": "eslint src/**/*.ts",
    "format": "prettier --write \"src/**/*.ts\"",
    "prepare": "npm run build",
    "prepublishOnly": "npm test && npm run lint",
    "preversion": "npm run lint",
    "version": "npm run format && git add -A src",
    "postversion": "git push && git push --tags",
    "publish-patch": "npm version patch && npm publish",
    "publish-minor": "npm version minor && npm publish",
    "publish-major": "npm version major && npm publish"
  },
  "keywords": [
    "whatsapp",
    "api",
    "sdk",
    "cloud-api",
    "messaging",
    "bot",
    "typescript",
    "webhook"
  ],
  "author": {
    "name": "Juu Studio",
    "url": "https://github.com/JuuStudio/"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/JuuStudio/whatsapp-sdk.git"
  },
  "bugs": {
    "url": "https://github.com/JuuStudio/whatsapp-sdk.git/issues"
  },
  "homepage": "https://github.com/JuuStudio/whatsapp-sdk.git#readme",
  "license": "MIT",
  "engines": {
    "node": ">=14.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.26.9",
    "@babel/preset-env": "^7.26.9",
    "@eslint/js": "^9.21.0",
    "@types/express": "^5.0.0",
    "@types/jest": "^29.5.14",
    "@types/node": "^16.18.126",
    "@typescript-eslint/eslint-plugin": "^5.62.0",
    "@typescript-eslint/parser": "^5.62.0",
    "babel-jest": "^29.7.0",
    "eslint": "^8.57.1",
    "globals": "^16.0.0",
    "jest": "^29.7.0",
    "prettier": "^2.8.8",
    "rimraf": "^5.0.10",
    "ts-jest": "^29.2.6",
    "typescript": "^4.9.5",
    "typescript-eslint": "^8.25.0"
  },
  "dependencies": {
    "axios": "^0.28.1"
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    },
    "./media": {
      "types": "./dist/media.d.ts",
      "default": "./dist/media.js"
    }
  }
}
